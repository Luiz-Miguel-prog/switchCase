    <script>
        var alunos = []
    
    for(var i = 0;i < 100;i++){
        alunos.push(i);
        console.log(alunos[i]);
        if(alunos[i] % 2 ==0) {
            console.log("PAR");}

        else{
            console.log("IMPAR");
        }
    }
    </script>
    
